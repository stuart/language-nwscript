	'preprocessor_directive': 'keyword.control.directive'

	'type_specifier': 'support.storage.type'

	'string_const': 'string.quoted.double'

	'''
	  'float',
	  'hex',
	  'integer'
	''' : 'constant.numeric'

	'''
	 "struct",
	 "if",
	 "else",
	 "switch",
	 "case",
	 'do_start',
	 "while",
	 "break",
	 "for",
	 'return'
	''' : 'keyword.control'

	'''
	  "=",
	  "*",
	  "/",
	  "+",
	  "-",
	  ">",
	  "<",
	  'sl',
	  'sr',
	  'usr',
	  "<",
	  ">",
	  'gteq',
	  'lteq',
	  'eq',
	  'noteq',
	  "&",
	  "^",
	  "|",
	  'andand',
	  'oror',
	  "?",
	  ":"
	''': 'keyword.operator'

	'comment': 'comment.block'

	'''
	'object_self_const',
	'object_invalid_const',
	'true_const',
	'false_const'
	''' : 'constant.language'

	'";"' : 'punctuation.terminator.statement'
	'"["' : 'punctuation.definition.begin.bracket.square'
	'"]"' : 'punctuation.definition.end.bracket.square'
	'","' : 'punctuation.separator.delimiter'
	'"{"' : 'punctuation.section.block.begin.bracket.curly'
	'"}"' : 'punctuation.section.block.end.bracket.curly'
	'"("' : 'punctuation.section.parens.begin.bracket.round'
	'")"' : 'punctuation.section.parens.end.bracket.round'

	'function_declarator > identifier:nth-child(1)' : 'entity.name.function'

 'identifier' : [
    {match: '^[A-Z0-9_][A-Z0-9_]*$', scopes: 'constant'},
    {match: '.*', scopes: 'variable'},
  ]

